{
  "name": "cartterr-portfolio-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Backend API for José Carter's portfolio website",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepare:images": "mkdir -p public/images"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5",
    "nodemailer": "^6.9.13"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/compression": "^1.7.5",
    "@types/nodemailer": "^6.4.14",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1",
    "tsx": "^4.7.0"
  },
  "keywords": [
    "portfolio",
    "api",
    "express",
    "typescript",
    "jose-carter"
  ],
  "author": "José Carter Arriagada",
  "license": "MIT"
}
